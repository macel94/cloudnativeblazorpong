    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>pongfreesqlserver</TargetDatabaseName>
    <DeployScriptFileName>Blazorpong.Database.AzureSql.sql</DeployScriptFileName>
